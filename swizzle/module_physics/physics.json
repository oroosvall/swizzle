{
    "kind": "StaticLib",
    "language": "c++",
    "files": [
        "../include/common/**.hpp",
        "include/**.hpp",
        "src/**.hpp",
        "src/**.cpp"
    ],
    "pub_inc_dirs": [
        "../include/",
        "include/"
    ],
    "vpaths": [
        {
            "name": "physics/*",
            "value": "src/**.hpp"
        },
        {
            "name": "physics/*",
            "value": "src/**.cpp"
        },
        {
            "name": "include/*",
            "value": "../include/**.hpp"
        },
        {
            "name": "include/*",
            "value": "include/**.hpp"
        }
    ],
    "buildflags": {
        "rtti": "Off"
    }
}
