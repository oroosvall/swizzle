{
    "kind": "ConsoleApp",
    "language": "c++",
    "files": [
        "test/**.hpp",
        "test/**.cpp",
        "include/**.hpp",
        "src/**.hpp",
        "src/**.cpp"
    ],
    "pub_inc_dirs": [
        "../include/",
        "include/",
        "test/",
        "src/"
    ],
    "vpaths": [
        {
            "name": "test/*",
            "value": "test/**.hpp"
        },
        {
            "name": "test/*",
            "value": "test/**.cpp"
        },
        {
            "name": "physics/*",
            "value": "src/**.hpp"
        },
        {
            "name": "physics/*",
            "value": "src/**.cpp"
        },
        {
            "name": "include/*",
            "value": "../include/**.hpp"
        },
        {
            "name": "include/*",
            "value": "include/**.hpp"
        }
    ],
    "buildflags": {
        "rtti": "Off"
    }
}
